pipeline {
    agent { label 'angular-sonnarscanner' }
    stages {
        stage('Clone') {
            steps {
                git branch: 'main',  url:  "${Giturl}"
            }
        }
        stage('Build') {
            steps {
                sh 'npm install'
                sh 'npx tsc index.ts --outDir dist'
            }
        }
        stage('Teste') {
            steps {
                sh 'npm test'
            }
        }
    }
}
